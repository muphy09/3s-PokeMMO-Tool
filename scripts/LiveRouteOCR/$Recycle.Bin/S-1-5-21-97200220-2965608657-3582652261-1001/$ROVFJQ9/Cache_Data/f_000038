    /* @__PURE__ */ jsxDEV("div", { style: { display: "flex", justifyContent: "space-between", alignItems: "baseline" }, children: [
      /* @__PURE__ */ jsxDEV("div", { className: "label-muted", children: [
        "Live Location: ",
        /* @__PURE__ */ jsxDEV("span", { style: { fontWeight: 800 }, children: rawText || "—" }, void 0, false, {
          fileName: "C:/dev/pokemmo-tool/src/App.jsx",
          lineNumber: 824,
          columnNumber: 26
        }, this),
        SHOW_CONFIDENCE && confPct !== null && /* @__PURE__ */ jsxDEV("span", { className: "text-slate-400 ml-2", children: [
          "(",
          confPct,
          "% Confidence)"
