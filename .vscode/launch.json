{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run App",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "electron:dev"],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    },
    {
      "name": "Run App - Debug",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "electron:dev:debug"],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    }
  ]
}
